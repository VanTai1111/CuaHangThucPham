@model IEnumerable<CuaHangThucPham.Models.CheckoutViewModel>

@{
    ViewBag.Title = "Danh sách đơn hàng";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header d-flex justify-content-between align-items-center">
                    <h3 class="card-title">Danh sách đơn hàng</h3>
                </div>
                <div class="card-body">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>ID Đơn Hàng</th>
                                <th>Người đặt</th>
                                <th>Ngày Đặt Hàng</th>
                                <th>Địa Chỉ Giao Hàng</th>
                                <th>Thành Phố</th>
                                <th>Tổng Tiền</th>
                                <th>Ghi Chú</th>
                                <th>Thao tác</th>
                            </tr>
                        </thead>

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
